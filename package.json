{
  "private": true,
  "devDependencies": {
    "coffeescript": "^2.5.1",
    "lerna": "^3.22.1",
    "mocha": "^8.1.3",
    "should": "^13.2.3"
  },
  "scripts": {
    "compile": "lerna run compile",
    "clean": "lerna run clean",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "update-version": "lerna publish --skip-git --skip-npm --force-publish '*'",
    "check-packages": "yarn clean && yarn compile && yarn test && yarn lint",
    "publish-packages": "./scripts/publish"
  },
  "workspaces": [
    "packages/*"
  ],
  "name": "parameters",
  "dependencies": {
    "@parametersjs/grpc_client": "file:packages/grpc_client",
    "@parametersjs/grpc_server": "file:packages/grpc_server",
    "parameters": "file:packages/parameters"
  }
}
