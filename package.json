{
  "name": "parameters",
  "version": "0.7.3",
  "description": "Command line arguments parser and stringifier",
  "keywords": [
    "argument",
    "arg",
    "parameter",
    "param",
    "option",
    "command",
    "cli",
    "getopt",
    "parser"
  ],
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "homepage": "https://github.com/adaltas/node-parameters",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "license": "MIT",
  "contributors": [
    "Sergei Kudinov <sergei@adaltas.com> (https://www.adaltas.com)"
  ],
  "engines": {
    "node": ">= 0.10.x"
  },
  "main": "./lib/index",
  "files": [
    "/lib"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/adaltas/node-parameters.git"
  },
  "dependencies": {
    "@grpc/grpc-js": "^0.3.6",
    "@grpc/proto-loader": "^0.5.0",
    "grpc": "^1.20.2",
    "mixme": "^0.3.1",
    "pad": "^2.3.0",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "coffeescript": "^2.4.1",
    "mocha": "^6.1.3",
    "should": "^13.2.3"
  },
  "optionalDependencies": {},
  "scripts": {
    "preversion": "grep '## Trunk' CHANGELOG.md && npm test",
    "version": "version=`grep '^  \"version\": ' package.json | sed 's/.*\"\\([0-9\\.]*\\)\".*/\\1/'` && sed -i \"s/## Trunk/## Version $version/\" CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push origin master && git push origin master --tags && npm publish",
    "patch": "npm version patch -m 'Bump to version %s'",
    "minor": "npm version minor -m 'Bump to version %s'",
    "major": "npm version major -m 'Bump to version %s'",
    "build": "coffee -b -o lib src && cp -rp src/plugins/grpc_server/shell.proto lib/plugins/grpc_server",
    "pretest": "npm run build",
    "test": "mocha test/{.,*}/*.coffee"
  }
}
