{
  "name": "@shell-js/grpc_server",
  "version": "0.9.0",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "contributors": [
    "Sergei Kudinov <sergei@adaltas.com> (https://www.adaltas.com)"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.4.4",
    "@shell-js/grpc_proto": "^0.9.0",
    "mixme": "^0.5.4",
    "protobufjs": "^6.11.2"
  },
  "devDependencies": {
    "coffeescript": "^2.6.1",
    "mocha": "^9.1.3",
    "should": "^13.2.3"
  },
  "description": "GRPC server plugin for the Shell.js package.",
  "engines": {
    "node": ">= 0.10.x"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/adaltas/node-shell",
  "keywords": [
    "argument",
    "parameter",
    "cli",
    "grpc",
    "client",
    "rpc",
    "remove",
    "server",
    "protocol",
    "interface"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should",
      "coffeescript/register"
    ],
    "inline-diffs": true,
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "peerDependencies": {
    "shell": "^0.8.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/adaltas/node-shell.git",
    "directory": "packages/grpc_server"
  },
  "scripts": {
    "preversion": "yarn run test",
    "build": "coffee -b -o lib src",
    "pretest": "yarn run build",
    "test": "mocha 'test/**/*.coffee'"
  }
}
