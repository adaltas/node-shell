{
  "name": "@parametersjs/grpc_client",
  "version": "0.8.0",
  "description": "GRPC client plugin for the parameters package.",
  "keywords": [
    "argument",
    "parameter",
    "cli",
    "grpc",
    "client",
    "rpc",
    "remove",
    "server",
    "protocol",
    "interface"
  ],
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/adaltas/node-parameters",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "license": "MIT",
  "contributors": [
    "Sergei Kudinov <sergei@adaltas.com> (https://www.adaltas.com)"
  ],
  "engines": {
    "node": ">= 0.10.x"
  },
  "main": "./lib/index",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@parametersjs/grpc_proto": "^0.8.0",
    "grpc": "^1.24.3",
    "protobufjs": "^6.10.1"
  },
  "devDependencies": {
    "@parametersjs/grpc_server": "^0.8.0"
  },
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should",
      "coffeescript/register"
    ],
    "inline-diffs": true,
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "peerDependencies": {
    "parameters": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/adaltas/node-parameters.git",
    "directory": "packages/grpc_client"
  },
  "scripts": {
    "preversion": "npm test",
    "version": "! grep '## Trunk' CHANGELOG.md && exit 0 || ( version=`grep '^  \"version\": ' package.json | sed 's/.*\"\\([0-9\\.]*\\)\".*/\\1/'` && sed -i \"s/## Trunk/## Version $version/\" CHANGELOG.md && git add CHANGELOG.md )",
    "__postversion": "git push origin master && git push origin master --tags && npm publish",
    "patch": "npm version patch -m 'Bump to version %s'",
    "minor": "npm version minor -m 'Bump to version %s'",
    "major": "npm version major -m 'Bump to version %s'",
    "build": "coffee -b -o lib src",
    "pretest": "npm run build",
    "test": "mocha 'test/**/*.coffee'"
  }
}
