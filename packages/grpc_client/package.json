{
  "name": "@shell-js/grpc_client",
  "version": "0.9.6",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "contributors": [
    "Sergei Kudinov <sergei@adaltas.com> (https://www.adaltas.com)"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1"
  },
  "description": "GRPC client plugin for the Shell.js package.",
  "devDependencies": {
    "@shell-js/grpc_server": "^0.9.6",
    "coffeescript": "^2.7.0",
    "mocha": "^10.7.0",
    "should": "^13.2.3"
  },
  "engines": {
    "node": ">= 0.10.x"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "/dist",
    "/lib"
  ],
  "homepage": "https://shell.js.org",
  "keywords": [
    "argument",
    "parameter",
    "cli",
    "grpc",
    "client",
    "rpc",
    "remove",
    "server",
    "protocol",
    "interface"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "publishConfig": {
    "access": "public"
  },
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should"
    ],
    "inline-diffs": true,
    "loader": "./test/loaders/coffee.js",
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "peerDependencies": {
    "shell": "^0.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/adaltas/node-shell.git",
    "directory": "packages/grpc_client"
  },
  "scripts": {
    "build": "npx rollup -c",
    "preversion": "yarn run test",
    "pretest": "npm run build",
    "test": "mocha 'test/**/*.coffee'"
  },
  "type": "module"
}
