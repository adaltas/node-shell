{
  "name": "@shell-js/grpc_proto",
  "version": "0.8.1",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "contributors": [
    "Sergei Kudinov <sergei@adaltas.com> (https://www.adaltas.com)"
  ],
  "dependencies": {
    "@grpc/proto-loader": "^0.5.5"
  },
  "description": "GRPC Protocol Buffer definition.",
  "devDependencies": {
    "coffeescript": "^2.5.1",
    "mocha": "^8.2.1",
    "should": "^13.2.3"
  },
  "engines": {
    "node": ">= 0.10.x"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/adaltas/node-shell",
  "keywords": [
    "argument",
    "parameter",
    "cli",
    "grpc",
    "client",
    "rpc",
    "remove",
    "protobuf",
    "protocol",
    "interface"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should",
      "coffeescript/register"
    ],
    "inline-diffs": true,
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/adaltas/node-shell.git",
    "directory": "packages/grpc_protobuf"
  },
  "scripts": {
    "preversion": "npm test",
    "version": "! grep '## Trunk' CHANGELOG.md && exit 0 || ( version=`grep '^  \"version\": ' package.json | sed 's/.*\"\\([0-9\\.]*\\)\".*/\\1/'` && sed -i \"s/## Trunk/## Version $version/\" CHANGELOG.md && git add CHANGELOG.md )",
    "patch": "npm version patch -m 'Bump to version %s'",
    "minor": "npm version minor -m 'Bump to version %s'",
    "major": "npm version major -m 'Bump to version %s'",
    "build": "coffee -b -o lib src && cp -rp src/shell.proto lib",
    "pretest": "npm run build",
    "test": "mocha 'test/**/*.coffee'"
  }
}
