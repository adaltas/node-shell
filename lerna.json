{
  "command": {
    "version": {
      "changelogPreset": {
        "name": "conventionalcommits",
        "issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/issues/{{id}}"
      },
      "conventionalCommits": true,
      "message": "chore(release): publish"
    }
  },
  "ignoreChanges": [
    "**/test/**"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "0.9.6"
}
